package asistenteVirtual.unidadesMedicion;

public class Gramo {

	private double cantidad;
	
	public Gramo(double cant) {
		this.cantidad = cant;
	}
	
	public void convertirA(Miligramo ml) {
		ml = new Miligramo(this.cantidad*1000);
	}
	
	public void convertirA(Kilo k) {
		k = new Kilo(this.cantidad/1000);
	}
}
