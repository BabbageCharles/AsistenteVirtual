<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-mapping>
	<class name="chat.Deuda" table="deudas">
	<composite-id>
		<key-property column="acreedor" name="acreedor" type="string" />
		<key-property column="deudor" name="deudor" type="string" />
	</composite-id>
		<property column="dinero" name="dinero" type="float" />
	</class>
</hibernate-mapping>